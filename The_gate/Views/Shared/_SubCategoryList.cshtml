@model List<SubCategory>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4 mb-3">
            <div class="card" style="width: 18rem;">
                <img src="@Url.Content(item.Image)" alt="no image" style="width:200px" />
                <div class="card-body">
                    <h5 class="card-title">SubCategories</h5>
                    <p class="card-text">@item.NameA</p>
                    <a href="#" class="btn btn-primary btn-load-prod" data-id="@item.Id">Products</a>
                </div>
            </div>
        </div>
    }
</div>
