@using Domain.Domains
@model List<SubCategory>
<table class="table table-bordered table-hover">
	<tr>
		<th>Name_En</th>
		<th>Name_Ar</th>
		<th>Image</th>
		<th>IsActive</th>
		<th>categoryId</th>
		<th>Edit</th>
		<th>Delete</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.NameE</td>
			<td>@item.NameA</td>
			<td><img src="@item.Image" alt="no image" style="width:100px" /></td>
			<td>@item.IsActive</td>
			<td>@item.category?.NameE </td>
			<td><a asp-action="Edit_SubCategory" asp-route-id="@item.Id" class="btn btn-primary">Edit</a></td>
			<td><a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a></td>

		</tr>
	}
</table>
<a asp-action="Add_SubCategory" class="btn btn-success btn-lg">AddSubCategory</a> 


