@model List<User>
@{
    ViewData["Title"] = "GetAllUsers";
}

<table class="table table-hover table-bordered">
    <tr>
        <th>Id</th>
        <th>UserName</th>
        <th>PassWord</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserName</td>
            <td>@item.PassWord</td>
        </tr>
    }

</table>
