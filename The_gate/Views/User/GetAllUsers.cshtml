@model List<User>
@{
    ViewData["Title"] = "GetAllUsers";
}

<table class="table table-hover table-bordered">
    <tr>
        <th>Id</th>
        <th>UserName</th>
        <th>PassWord</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserName</td>
            <td>@item.PassWord</td>
            <td><a asp-controller="User" asp-action="Edit" asp-route-id="@item.Id" class="btn btn-success">Edit</a></td>
            <td><a asp-controller="User" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a></td>
        </tr>
    }

</table>
